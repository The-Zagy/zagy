{
  "name": "base",
  "version": "1.0.0",
  "description": "node-js base template no flavors at all",
  "type": "module",
  "keywords": [],
  "main": "dist/index.js",
  "scripts": {
    "start": "node  ./dist/index.js",
    "build": "tsc",
    "build:watch": "tsc --watch --preserveWatchOutput",
    "dev": "nodemon  dist/index",
    "ts": "ts-node src/index.ts",
    "lint": "eslint . --ext .ts",
    "format": "eslint src/**/*.ts --fix",
    "prettier": "prettier --config .prettierrc \"src/**/*.ts\" --write"
  },
  "author": "the-zagy",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.6.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {}
}
